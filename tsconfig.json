{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "sourceMap": true,
    "declaration": true,
    "noResolve": true,
    "stripInternal": true,
    "declarationDir": "types",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "outDir": "dist"
  },
  "exclude": [
    "node_modules",
    "dist",
    "types"
  ]
}